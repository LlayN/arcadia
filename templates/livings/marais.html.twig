                                                            method="get">
                                                            <button type="submit"
                                                                class="btn z-3 text-white fw-bold p-0"
                                                                data-bs-toggle="tooltip"
                                                                data-bs-placement="top"
                                                                data-bs-title="Détail de l'animal">
