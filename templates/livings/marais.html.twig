                                    <div class="offcanvas offcanvas-start"
                                        tabindex="1"
                                        id="{{ animal.id }}"
                                        aria-labelledby="offcanvasExampleLabel">
                                        <div class="offcanvas-header">
                                            <button type="button"
                                                class="btn-close"
                                                data-bs-dismiss="offcanvas"
                                                aria-label="Close">

                                            </button>
                                        </div>

