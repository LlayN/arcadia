                                                    <h2 class="fw-bold text-white m-0 text-uppercase">
                                                        {{ animal.name }}
                                                    </h2>
                                                    <a class="btn p-0"
                                                        data-bs-toggle="offcanvas"
                                                        href="#{{ animal.id }}"
                                                        role="button"
                                                        aria-controls="offcanvasExample"
                                                        onclick="statsView({{
                                                        animal.id
                                                        }})">
