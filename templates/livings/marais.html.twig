                                            <ul class="d-flex flex-column gap-4">
                                                <li>
                                                    <div class="d-flex align-items-center gap-2">
                                                        <h5 class="fw-semibold m-0 text-uppercase">
                                                            {{ animal.name }}
                                                        </h5>
                                                    </div>
                                                </li>
                                                <span class="little-line-article bg-warning mt-2">

                                                </span>
                                                <li>
                                                    <div class="d-flex flex-column flex-sm-row align-items-sm-center gap-2">
                                                        <h6 class="fw-semibold m-0">
                                                            Race :
                                                        </h6>
                                                        <p class="m-0 txt-1">
                                                            {{ animal.race }}
                                                        </p>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="d-flex flex-column flex-sm-row align-items-sm-center gap-2">
                                                        <h6 class="fw-semibold m-0">
                                                            Régime Alimentaire :
                                                        </h6>
                                                        <p class="m-0 txt-1">
                                                            {% if
                                                                animal.veterinaryReport is not null %}
                                                                {{
                                                                    animal.veterinaryReport.food
                                                                }}
                                                            {% else %}
                                                                <p class="m-0 txt-1">
                                                                    Aucune
                                                                    donnée.
                                                                </p>
                                                            {% endif %}
                                                        </p>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="d-flex flex-column flex-sm-row align-items-sm-center gap-2">
                                                        <h6 class="fw-semibold m-0">
                                                            Quantité Distribuée
                                                            :
                                                        </h6>
                                                        <p class="m-0 txt-1">
                                                            {% if
                                                                animal.veterinaryReport is not null %}
                                                                {{
                                                                    animal.veterinaryReport.quantity
                                                                }}

                                                                kg/j
                                                            {% else %}
                                                                <p class="m-0 txt-1">
                                                                    Aucune
                                                                    donnée.
                                                                </p>
                                                            {% endif %}
                                                        </p>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="d-flex flex-column flex-sm-row align-items-sm-center gap-2">
                                                        <h6 class="fw-semibold m-0">
                                                            État de Santé :
                                                        </h6>
                                                        <p class="m-0 txt-1">
                                                            {% if
                                                                animal.veterinaryReport is not null %}
                                                                {{
                                                                    animal.veterinaryReport.state
                                                                }}
                                                            {% else %}
                                                                <p class="m-0 txt-1">
                                                                    Aucune
                                                                    donnée.
                                                                </p>
                                                            {% endif %}
                                                        </p>
                                                    </div>
                                                </li>
                                                <li>
                                                    <div class="d-flex flex-column flex-sm-row align-items-sm-center gap-2">
